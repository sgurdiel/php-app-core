{
    "name": "xver/php-app-core",
    "type": "symfony-bundle",
    "description": "Provides a PHP skeleton for developing new apps",
    "keywords": [],
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^8.4",
        "doctrine/collections": "^2.1",
        "symfony/translation": "^8.0",
        "symfony/http-kernel": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "vimeo/psalm": "^6.10",
        "symfony/dependency-injection": "^8.0",
        "symfony/config": "^8.0"
    },
    "autoload": {
        "psr-4": { "Xver\\PhpAppCoreBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Xver\\PhpAppCoreBundle\\Tests\\": "tests/" }
    }
}
